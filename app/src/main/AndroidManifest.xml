<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tasktracdemo2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TaskTracDemo2">
        <activity android:name=".Activity.TestActivity" />
        <activity android:name=".Activity.OthersActivity" />
        <activity android:name=".Activity.CourseworkActivity" />
        <activity android:name=".Activity.ExamsActivity" />
        <activity android:name=".Activity.CategoryActivity" />
        <activity android:name=".Activity.BaseActivity" />
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".HelperAndHandler.AlertReceiverHandler" />
    </application>

</manifest>